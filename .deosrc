include .env
#[path]
DOTVENV    := $(V)/.venv
DOTSWAP    := $(V)/.swap
SWAPWIKI   := $(DOTSWAP)/wiki
VAR        := $(V)/var
VARWIKI    := $(VAR)/wiki
#[github]
COREREPO   := git@github.com:libdeos/deos-core.git
COREWIKI   := git@github.com:libdeos/deos-core.wiki.git
WIKIMSG    := deos: wiki.sync
#[python]
ACTVENV    := source $(DOTVENV)/bin/activate
PIPINSTALL := pip install
PY         := python
PYREQ      := requirements.txt
VENV       := virtualenv --always-copy --no-site-packages
#[endfi]
